			      C __paramLoop2a:
			      C     movd      mm1, [eax+vertex]     ; get next parameter
			      C     mov       eax, [dlp]            ; offset = *(dlp + 1)
			      C 
			      C     cmp       eax, 0                ; at end of offset list (offset == 0) ?
