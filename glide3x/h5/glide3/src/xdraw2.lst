			      C 
			      C     mov       edx, [gc + fifoRoom]              ; old fifo space available
			      C     inc       ecx                               ; _GlideRoot.stats.trisDrawn++
			      C 
			      C     mov       esi, [gc + trisProcessed]         ; _GlideRoot.stats.trisProcessed
